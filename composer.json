{
    "name": "intervention/validation",
    "description": "Additional Validator Functions for the Laravel Framework",
    "keywords": ["validation", "Validator", "laravel", "bic", "iban", "creditcard", "isbn", "isin", "luhn"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Vogel",
            "email": "oliver@olivervogel.com",
            "homepage": "https://olivervogel.com/"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^5.5|~6|~7|~8",
        "illuminate/validation": "^5.5|~6|~7|~8"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "phpstan/phpstan": "^0.12.17"
    },
    "autoload": {
        "psr-4": {
            "Intervention\\Validation\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Intervention\\Validation\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Intervention\\Validation\\Laravel\\ValidationServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
